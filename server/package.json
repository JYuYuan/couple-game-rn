{
  "name": "couple-game-server",
  "version": "1.0.0",
  "description": "Socket.io server for couple game",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "simulator": "node simulator-cli.js",
