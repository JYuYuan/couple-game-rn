  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}