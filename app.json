{
  "expo": {
    "name": "卿轻游",
    "slug": "couple-game-rn",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "couplegamern",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coupleup.game",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.coupleup.game"
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "build": {
        "babel": {
          "include": []
        }
      },
      "favicon": "./assets/images/favicon.png",
      "name": "情侣游戏合集 | 飞行棋·扫雷·转盘·趣味任务",
      "shortName": "情侣游戏",
      "description": "专为情侣设计的多人互动游戏平台！包含经典飞行棋、刺激扫雷、幸运大转盘和趣味18+任务。支持在线联机和局域网对战。",
      "lang": "zh-CN",
      "scope": "/",
      "themeColor": "#ff6b9d",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "dir": "ltr",
      "preferRelatedApplications": false,
      "barStyle": "black-translucent",
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent",
        "apple-mobile-web-app-title": "情侣游戏"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "6360c8ec-915d-40b7-ad9b-35a34b90a968"
      }
    },
    "owner": "jiangyuyu",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": false
    }
  }
}
